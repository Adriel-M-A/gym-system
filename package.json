    "start": "electron .",
    "build:main": "tsup",
    "build:renderer": "vite build",
    "build": "npm run build:main && npm run build:renderer && powershell -Command \"Copy-Item -Path 'main/database/migrations' -Destination 'dist-electron/migrations' -Recurse -Force\"",
    "rebuild": "electron-rebuild"
  },
