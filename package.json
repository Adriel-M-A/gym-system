  "main": "dist-electron/main.js",
  "scripts": {
    "dev:vite": "vite",
    "dev:backend": "wait-on tcp:3000 && tsup --watch --onSuccess \"powershell -Command \\\"Copy-Item -Path 'main/database/migrations' -Destination 'dist-electron' -Recurse -Force\\\" && electron .\"",
